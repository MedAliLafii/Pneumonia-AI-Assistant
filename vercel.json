{
  "version": 2,
  "functions": {
    "app.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "PYTHON_VERSION": "3.11"
  }
}
